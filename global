                    if #selectedMerchantItem > 0 then
                        local toBuy = {}

                            
                            for _, merchName in ipairs(merchlist) do
                                if merchName ~= "All" then
                                end
                            end
                        else
                            toBuy = selectedMerchantItem
                        end

                        
